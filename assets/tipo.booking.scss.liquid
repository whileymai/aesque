                    .title{
                        font-size:18px;
                    }
                }
                .booking-info {
                    padding : 15px;
                    flex-direction: column;
                    align-items: flex-start;
                    > div{
                        border-bottom: .1rem solid #E7EAF4;
                        width: 100%;
                        margin-bottom: 5px;
                        padding-bottom: 5px;
                        display: flex;
                        justify-content: space-between;
                        &:last-child{
                            margin-bottom: 0px;
                            padding-bottom: 0px;
                            border-bottom: none;
                        }
                    }
                }
                .extra-fields{
                    padding : 15px;
                    .element{
                        flex: 0 0 100%;
                        border:none;
                        margin-bottom: 10px;
                        &:last-child{
                        margin-bottom: 0px;
                        }
                    }
                    .tpb-checkbox{
                        display:block;
                    }
                    .tpb-radio-group_item {
                        text-align: left;
                    }
                }
                .subtotal{
                    padding : 15px;
                }
            }
        }
    }
    .tpb-datepicker{
    .datepicker-panel{
      >ul{
          >li{
              height:30px;
              line-height:30px;
              border-radius:0;
          }
      }
    }
  }
  .action{
    padding: 15px;
    .back-button{
        width:100%;
        margin-bottom: 15px;
        &.c2{
        display:block;
        }
    }
    .confirm-button{
        width:100%;
        margin-bottom: 0;
    }
  }
}
