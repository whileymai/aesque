        .content{
            padding: 0;
        	width: 300%;
          	transition: all .5s;
            ul li:before {
              content: unset;
            }
          	.step1{
              	width: 33.3333333%;
              	float:left;
                transition: all 0.5s;
              	background:#fff;
              	padding: 65px 40px 65px 30px;
                background-image: url($background_image);
                background-repeat: no-repeat;
                @include respond-to(mobile-portrait){
                  padding:25px;
                }
              	.inner-step{
                  	width:50%;
                  	margin:0 auto;
                  	transition:all .5s;
                    float:right;
                  	@include respond-to(mobile-portrait){
                      width:100%;
                    }
                  	h5{
                      font-size: 24px;
                      color: $primaryTextColor;
                      font-weight: 400;
                	}
                }
            }
            .step2{
				        width: 33.3333333%;
              	float:left;
                transition: all 0.5s;
                background: $calender_background_color;
              	padding: 65px 50px 65px 50px;
                z-index:9;
                @include respond-to(mobile-portrait){
                	padding: 25px;
                }
            }
            .step3{
            	width: 33.333333%;
              	float:left;
                transition: all 0.5s;
              	background:#fff;
            }
            &[data-step="1"]{
                .step1{}
                .step2{height:0;}
              	.step3{height:0;}
            }
          	&[data-step="2"]{
              	width:200%;
                .step1{
                  width:25%;
                  height:0;
                  background-image: unset;
                  .inner-step{
                  	width:100%;
                  }
                }
                .step2{width:25%;}
                .step3{width:50%;height:0;}
                @include respond-to(mobile-portrait){
                	width:300%;
                  	transform:translate(-33.3333%);
                    .step1{width:33.3333%;height:0;}
                    .step2{width:33.3333%;}
                    .step3{width:33.3333%;height:0;}
                }
            }
          	&[data-step="3"]{
              	width:200%;
              	transform:translate(-50%);
                .step1{width:25%;height:0;}
                .step2{width:25%;height:0;}
                .step3{width:50%;}
                @include respond-to(mobile-portrait){
                	width:300%;
                    transform:translate(-66.6667%);
                    .step1{width:33.3333%;height:0;}
                    .step2{width:33.3333%;height:0;}
                    .step3{width:33.3333%;}
                }
            }
        }

