/*================ ANIMATION ================*/
/**
 * 
 */

$dur: {{ settings.animation }}s;
/**
 * 
 */

//general
a, [class^="icon-"] {
  @include transition(color $dur);
}


//modifiers
.ttg-text-animation-parent .ttg-text-animation--emersion > * {
  @include transition(transform ease $dur);
}
.ttg-image-translate--left,
.ttg-image-translate--right,
.ttg-image-translate--top,
.ttg-image-translate--bottom,
.ttg-image-scale {
  img {
    @include transition(transform ease $dur);
  }
}
.ttg-transform-in-load,
.ttg-loading::after {
  @include transition(opacity $dur);
}


//buttons, inputs
input,
textarea,
select,
button,
.btn,
.tt-btn {
  -webkit-transition: background-color $dur, border-color $dur, color $dur;
  -moz-transition: background-color $dur, border-color $dur, color $dur;
  -ms-transition: background-color $dur, border-color $dur, color $dur;
  -o-transition: background-color $dur, border-color $dur, color $dur;
  transition: background-color $dur, border-color $dur, color $dur;
}
.btn,
.tt-btn {
  & > span,
  & > i {
    @include transition(color $dur);
  }
  &.tt-btn--hover-effect::after {
    -webkit-transition: -webkit-transform 0.3s, opacity $dur;
    -moz-transition: -moz-transform 0.3s, opacity $dur;
    transition: transform 0.3s, opacity $dur;
  }
}


//color
.tt-cart__product_info a p,
.tt-wishlist__product_info a p,
.tt-counter__inner .tt-counter__control span,
.tt-layer-nav__product-info a > p,
.tt-layer-nav__prod-list-section a > p {
  @include transition(color $dur);
}


//border
.tt-cart__product_image img,
.tt-wishlist__product_image img,
.tt-header__search-dropdown img,
.tt-header__cart-list ul li img,
.tt-layer-nav__product-image {
  @include transition(border-color $dur);
}


//background-color
.tt-header__user-list > ul li,
.tt-header__settings-list > ul li {
  @include transition(background-color $dur);
}


//transform
.tt-categories__next {
  @include transition(transform $dur);
}


//all
.tt-footer__to-top,
.tt-header__btn-info i,
.tt-newsletter .btn,
.tt-categories:not(.tt-categories__single) li a::before {
  @include transition($dur);
  border: solid 1px #2d2d2d;
}

.tt-header--build-01,
.tt-header--build-04 {
  .tt-header__btn > span {
    @include transition(0.1s);
  }
}


//other
.tt-contacts__map iframe {
  @include transition(filter $dur);
}

.tt-carousel-brands .slick-slide {
  @include transition(opacity $dur);
}

.tt-gallery__slider {
  .swiper-btn-next,
  .swiper-btn-prev {
    @include transition($dur);
  }
  .gallery-thumbs {
    .swiper-slide img {
      @include transition($dur);
    }
  }
}



