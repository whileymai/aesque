    ol {
      & > li::before {
        color: $theme;
      }
    }
    input[type='text'],
    input[type='search'],
    input[type='password'],
    textarea,
    select {
      border-color: $theme-transparent;
      &:focus {
        border-color: $theme;
      }
      &::-webkit-input-placeholder { color: $theme-c; }
      &::-moz-placeholder { color: $theme-c; }
      &:-ms-input-placeholder { color: $theme-c; }
      &:-moz-placeholder { color: $theme-c; }
    }
    textarea,
    select {
      color: $theme-c;
    }
    input,
    textarea,
    select {
      background-color: $theme6;
    }
    select:focus {
      border-color: $theme-transparent;
    }
    table tr {
      &:not(:first-child) {
        td {
          border-top-color: $theme8;
        }
      }
      th {
        border-bottom-color: $theme8;
        color: $theme-head-c;
      }
    }
    .icon {
      fill: $theme-icon-c;
      path {
        fill: $theme-icon-c;
      }
    }
  }


  //rtl
  [dir="rtl"] {
    .tt-tabs[data-tt-type="vertical"] .tt-tabs__head {
      @media only screen and (min-width: $screen-md) {
