//global
body {
  background-color: $theme-body;
  color: $theme-c;
}
h1, h2, h3, h4, h5, h6 {
  color: $theme-head-c;
}
a {
  color: $label-c;
  &:visited,
  &:active,
  &:link {
    color: $theme3;
  }
  &:hover {
    @media only screen and (min-width: $screen-md) {
      color: $label-h;
    }
  }
}
p {
  color: $theme-paragraph-c;
}
blockquote {
  border-color: $theme8;
}
[class^="icon-"] {
  color: $theme-icon-act-c;
  &.active {
    color: $theme-icon-act-c;
  }
}
a:hover > [class^="icon-"] {
  @media only screen and (min-width: $screen-md) {
    color: $theme-icon-act-c;
  }
}

#theme {
  //modifiers
  .ttg-text--highlight {
    background-color: $theme;
    color: $theme2;
  }
  .ttg-quote {
    p, span {
      color: $theme4;
    }
    a {
      @include _link-color($theme4);
    }
    i {
      color: $theme;
    }
  }
  .ttg-capitalize {
    &:first-letter {
      background-color: $theme9;
      color: $theme2;
    }
  }
  .ttg__required {
    &::after {
      content: ' *';
      color: $theme-error;
    }
  }
  .ttg-grid-border {
    & > [class^="col-"]:not(:first-child) {
      @media only screen and(min-width: $screen-md-down) {
        border-color: $theme8;
      }
    }
  }
  ul.ttg-list,
  ol.ttg-list {
    & > li {
      &::before {
        background-color: $theme;
      }
      li::before {
        background-color: $theme-head-c;
      }
    }
  }
  .rte {
    ul {
      & > li {
        &::before {
          background-color: $theme;
        }
        li::before {
          background-color: $theme-head-c;
        }
      }
    }
  }


  //animations
  .ttg-animation-icon--wave i::after {
    border-color: $theme;
  }
  .ttg-loading,
  .tt-preloader {
    &::before {
      border-top-color: $theme;
      border-bottom-color: $theme;
      border-left-color: $theme8;
      border-right-color: $theme8;
    }
    &::after {
      background-color: $theme2;
    }
  }


  //input
  input[type='text'],
  input[type='email'],
  input[type='search'],
  input[type='password'],
  input[type='tel'],
  textarea,
  select {
    background-color: $input-bg;
    border-color: $input-bd;
    color: $input-c;
    &::-webkit-input-placeholder { color: $input-c; }
    &::-moz-placeholder { color: $input-c; }
    &:-ms-input-placeholder { color: $input-c; }
    &:-moz-placeholder { color: $input-c; }
    &:focus {
      background-color: $input-f-bg;
      border-color: $input-f-bd;
    }
  }
  .tt-counter__control {
    span:hover {
      @media only screen and (min-width: $screen-md) {
        color: $theme;
      }
    }
  }
  .tt-checkbox-circle span {
    border-color: $theme7;
  }


  //buttons
  .btn:not([class*="colorize-btn"]):not([class*="colorize-btn1"]),
  .colorize-btn,
